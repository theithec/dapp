        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.thtp.dapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="de.thtp.dapp.DiagramActivity" />
        <activity android:name="de.thtp.dapp.DappPreferencesActivity" />
        <activity android:name="de.thtp.dapp.PlayerListActivity" />
        <activity android:name="de.thtp.dapp.SessionListActivity" />
        <activity android:name="de.thtp.dapp.SessionPlayersActivity" />
        <activity android:name="de.thtp.dapp.SessionResultActivity" />
        <activity android:name="de.thtp.dapp.GameResultAddGameActivity" />
        <activity android:name="de.thtp.dapp.GamePlayersAddGameActivity" />
        <activity android:name="de.thtp.dapp.GameResultEditGameActivity" />
